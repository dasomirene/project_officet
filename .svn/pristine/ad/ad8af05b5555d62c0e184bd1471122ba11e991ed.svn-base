package officet.officet_vo;

// 승인자
public class Approver {

	// 필드
	private int approver_id;	// 승인자 id
	private int worker_no;		// 사원 id
	
	// 생성자
	public Approver() {}
	
	public Approver(int approver_id, int worker_no) {
		this.approver_id = approver_id;
		this.worker_no = worker_no;
	}
	
	// getter / setter
	public int getApprover_id() {
		return approver_id;
	}
	public void setApprover_id(int approver_id) {
		this.approver_id = approver_id;
	}
	public int getWorker_no() {
		return worker_no;
	}
	public void setWorker_no(int worker_no) {
		this.worker_no = worker_no;
	}
}
