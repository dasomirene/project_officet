package backendWeb.z01_vo;

public class Dept01 {
private int deptno;
private String dname;
private String job;
public Dept01() {
	super();
	// TODO Auto-generated constructor stub
}
public Dept01(int deptno, String dname, String job) {
	super();
	this.deptno = deptno;
	this.dname = dname;
	this.job = job;
}
public int getDeptno() {
	return deptno;
}
public void setDeptno(int deptno) {
	this.deptno = deptno;
}
public String getDname() {
	return dname;
}
public void setDname(String dname) {
	this.dname = dname;
}
public String getJob() {
	return job;
}
public void setJob(String job) {
	this.job = job;
}

}
