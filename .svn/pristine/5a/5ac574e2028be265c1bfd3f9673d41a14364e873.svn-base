package backendWeb.z01_vo;
public class Part {
private int pNum;
private String pName;
private String pKind;
private String company;
private int price;

public Part() {
	// TODO Auto-generated constructor stub	
}


public Part(int pNum, String pKind) {
	this.pNum = pNum;
	this.pKind = pKind;
}

public Part(int pNum, String pName, String pKind, String company, int price) {
	this.pNum = pNum;
	this.pName = pName;
	this.pKind = pKind;
	this.company = company;
	this.price = price;
}
public int getpNum() {
	return pNum;
}
public void setpNum(int pNum) {
	this.pNum = pNum;
}
public String getpName() {
	return pName;
}
public void setpName(String pName) {
	this.pName = pName;
}
public String getpKind() {
	return pKind;
}
public void setpKind(String pKind) {
	this.pKind = pKind;
}
public String getCompany() {
	return company;
}
public void setCompany(String company) {
	this.company = company;
}
public int getPrice() {
	return price;
}
public void setPrice(int price) {
	this.price = price;
}

}
