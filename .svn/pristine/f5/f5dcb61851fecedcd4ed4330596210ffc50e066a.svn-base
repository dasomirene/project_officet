package officet.officet_vo;

import java.util.Date;

// 결재
public class Approval {

	// 필드
	private int approval_no;		// 결재 id
	private int worker_no;			// 사원 id
	private int approval_approver_no;		// 승인자 id
	private int approval_step;			// 결재 단계
	private int approval_status;		// 결재 상태
	private Date approval_request_time;		// 결재 요청 시간
	
	// 생성자
	public Approval() {}
	public Approval(int approval_no, int worker_no, int approval_approver_no, int approval_step, int approval_status,
			Date approval_request_time) {
		this.approval_no = approval_no;
		this.worker_no = worker_no;
		this.approval_approver_no = approval_approver_no;
		this.approval_step = approval_step;
		this.approval_status = approval_status;
		this.approval_request_time = approval_request_time;
	}
	
	// getter / setter
	public int getApproval_no() {
		return approval_no;
	}
	public void setApproval_no(int approval_no) {
		this.approval_no = approval_no;
	}
	public int getWorker_no() {
		return worker_no;
	}
	public void setWorker_no(int worker_no) {
		this.worker_no = worker_no;
	}
	public int getApproval_approver_no() {
		return approval_approver_no;
	}
	public void setApproval_approver_no(int approval_approver_no) {
		this.approval_approver_no = approval_approver_no;
	}
	public int getApproval_step() {
		return approval_step;
	}
	public void setApproval_step(int approval_step) {
		this.approval_step = approval_step;
	}
	public int getApproval_status() {
		return approval_status;
	}
	public void setApproval_status(int approval_status) {
		this.approval_status = approval_status;
	}
	public Date getApproval_request_time() {
		return approval_request_time;
	}
	public void setApproval_request_time(Date approval_request_time) {
		this.approval_request_time = approval_request_time;
	}
	
}
